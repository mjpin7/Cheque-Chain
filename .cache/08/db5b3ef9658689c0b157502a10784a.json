n            [...Array(Math.ceil(this.state.blocksLength/5)).keys()].map(key => {\r\n              const paginatedId = key+1;\r\n\r\n              return (\r\n                <span key={key} onClick={this.fetchPaginatedBlocks(paginatedId)}>\r\n                  <Button bsSize=\"small\" bsStyle=\"danger\">\r\n                    {paginatedId}\r\n                  </Button>{' '}\r\n                </span>\r\n              )\r\n            })\r\n          }\r\n        </div>\r\n        {\r\n          this.state.blocks.map(block => {\r\n            return (\r\n              <Block key={block.hash} block={block} />\r\n            );\r\n          })\r\n        }\r\n      </div>\r\n    );\r\n  }\r\n}\r\n\r\nexport default Blocks;"},"lineCount":null}},"error":null,"hash":"e0fb8074802c5869a57bfeac7b779a87","cacheData":{"env":{}}}